<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="SpiceJet">
	<test name="Valid Positive Test Cases">
		<parameter name="browser" value="Chrome"></parameter>
		<!--<parameter name="browser" value="Edge"></parameter>-->
		<classes>
			<class name="com.SpiceJet.test.SignUpTest"></class>
			<class name="com.SpiceJet.test.LoginTest_DataProvider" />
			<class name="com.SpiceJet.test.OneWayTest"></class>
			<class name="com.SpiceJet.test.RoundTripTest" />
			<class name="com.SpiceJet.test.FieldsAvailableTest" />
			<class name="com.SpiceJet.test.PayementSpiceJet" />
		</classes>
	</test>
	<test name="InValid Negative Test cases">
		<parameter name="browser" value="Chrome"></parameter>
		<classes>
			<class name="com.SpiceJet.test.NeagtiveTc_SignUp" />
			<class name="com.SpiceJet.test.NegativeTc_Login_DataProvider" />
		</classes>

	</test>


</suite> 
